<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.apache.ibatis.demo.UserDao">
    <select id="selectUsers" resultType="org.apache.ibatis.demo.User">
        select * from users
    </select>

    <select id="selectByName" resultType="org.apache.ibatis.demo.User">
        select * from users where name = #{name}
    </select>

</mapper>